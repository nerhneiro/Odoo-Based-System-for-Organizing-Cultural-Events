            <field name="padding">5</field>
        </record>

        <record id="sequence_management_speaker" model="ir.sequence">
            <field name="name">Speaker sequence</field>
            <field name="code">management.speaker</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>