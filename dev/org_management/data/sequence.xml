        <record id="sequence_management_guest" model="ir.sequence">
            <field name="name">Guest sequence</field>
            <field name="code">management.guest</field>
            <field name="prefix">G</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_management_vip_guest" model="ir.sequence">
            <field name="name">VIPguest sequence</field>
            <field name="code">management.vip_guest</field>
